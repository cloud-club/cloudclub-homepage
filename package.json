{
  "name": "cloudclub-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^13.5.0",
    "gh-pages": "^4.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn watch:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build",
    "predeploy": "npm run build",
    "watch:css": "postcss src/tailwind.css -o src/global.css",
    "watch": "chokidar \"./src/**/*.jsx\" -c \"yarn watch:css\"",
    "dev": "concurrently \"yarn watch\" \"yarn start\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "http://cloud-club.github.io/cloudclub-homepage",
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.2.2",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "postcss-loader": "^7.0.0",
    "tailwindcss": "^3.1.4"
  },
  "eslintIgnore": [
    "reportWebVitals.js"
  ]
}
